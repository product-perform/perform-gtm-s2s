}
var cid;
function getCidFromUrl(u) {
  return u.match('(?:^|[^0-9])([0-9]{29})(?![0-9])');
}

var clickIdPresentInUrl = false;
if (url) {
   cid = getCidFromUrl(url);
  if (cid)
    cid=cid.length>1?cid[1]:cid[0];
   if (cid)
     clickIdPresentInUrl = true;
}
if (!cid){
    cid = getCookieValues('_pmedia_clid')[0];
    if (cid && cid.indexOf(".")>-1)
       cid= cid.split('.')[0];
    if (cid)
      cid = cid.split(':')[1];
}

var requestUrl = getRequestUrl(eventData);

if (isDebug) {
    logToConsole(JSON.stringify({
        'Name': 'Perform',
        'Type': 'Request',
        'EventName': eventData.event_name,
        'RequestMethod': 'GET',
        'RequestUrl': requestUrl,
    }));
}

sendHttpRequest(requestUrl, (statusCode, headers, body) => {
    if (isDebug) {
        logToConsole(JSON.stringify({
            'Name': 'Perform',
            'Type': 'Response',
            'EventName': eventData.event_name,
            'ResponseStatusCode': statusCode,
            'ResponseHeaders': headers,
            'ResponseBody': body,
        }));
    }
  
    if (clickIdPresentInUrl && eventData.event_name === 'page_view') {
            setCookie('_pmedia_clid', data.advertiserKey+ ":" + cid, {
                domain: 'auto',
                path: '/',
                samesite: 'Lax',
                secure: true,
                'max-age': 63072000, // 2 years
                httpOnly: false
            });
        }
  
    if (statusCode >= 200 && statusCode < 300)
        data.gtmOnSuccess();
    else
        data.gtmOnFailure();
}, {headers: {'user-agent': eventData.user_agent},method: 'GET'});

function getRequestUrl(evenData)
{
    var requestUrl = 'https://c.pmsrv.co/v2/sscvr?akey=' + data.advertiserKey;
    if (cid)
        requestUrl+='&cid='+encodeUriComponent(cid);
    if (eventData.event_name){
        if (eventData.event_name === 'page_view')
          requestUrl+= '&event=page-land&weight=0';
        else
          requestUrl+='&event='+encodeUriComponent(eventData.event_name);
    }
    if (eventData.ip_override)
      requestUrl+='&ip='+encodeUriComponent(eventData.ip_override);
    return requestUrl;
}


